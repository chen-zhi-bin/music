<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chan.music.mapper.SingerDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.chan.music.pojo.Singer">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="sex" property="sex" />
        <result column="pic_id" property="picId" />
        <result column="birth" property="birth" />
        <result column="location" property="location" />
        <result column="introduction" property="introduction" />
        <result column="music_count" property="musicCount" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, `name`, sex, pic_id, birth, location, introduction, music_count
    </sql>

</mapper>
