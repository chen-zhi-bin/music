<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chen.music.mapper.MusicDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.chen.music.pojo.Music">
        <id column="id" property="id" />
        <result column="singer_id" property="singerId" />
        <result column="name" property="name" />
        <result column="introduction" property="introduction" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="pic_id" property="picId" />
        <result column="lyric" property="lyric" />
        <result column="url" property="url" />
        <result column="state" property="state" />
        <result column="md5" property="md5" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, singer_id, `name`, introduction, create_time, update_time, pic_id, lyric, url, state, md5
    </sql>

</mapper>
